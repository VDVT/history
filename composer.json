{
    "name": "vdvt/history",
    "description": "write history via hook eloquent",
    "authors": [
        {
            "name": "Trinh Le",
            "email": "lehuutrinh32@gmail.com"
        }
    ],
    "homepage": "https://github.com/vdvt/history",
    "keywords": ["Laravel", "History"],
    "require": {
        "illuminate/support": "~5",
        "vdvt/support": "~1.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "VDVT\\History\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VDVT\\History\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VDVT\\History\\HistoryServiceProvider"
            ],
            "aliases": {
                "History": "VDVT\\History\\Facades\\History"
            }
        }
    }
}
